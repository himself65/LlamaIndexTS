{
  "name": "@llamaindex/readers",
  "version": "0.0.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/himself65/LlamaIndexTS.git",
    "directory": "packages/readers"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "bunchee",
    "dev": "bunchee --watch"
  },
  "devDependencies": {
    "@types/papaparse": "^5.3.14",
    "bunchee": "^4.4.8"
  },
  "dependencies": {
    "@llamaindex/core": "workspace:^",
    "@notionhq/client": "^2.2.14",
    "assemblyai": "^4.2.2",
    "file-type": "^18.7.0",
    "llamaindex": "workspace:*",
    "mammoth": "^1.6.0",
    "mongodb": "^6.3.0",
    "notion-md-crawler": "^0.0.2",
    "papaparse": "^5.4.1",
    "pdf2json": "^3.0.5",
    "string-strip-html": "^13.4.6"
  }
}
